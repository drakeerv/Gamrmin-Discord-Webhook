<resources>
	<properties>
		<property id="webhookUrl" type="string">https://example.com/</property>
		<property id="username" type="string">Example</property>
		<property id="avatarUrl" type="string">https://example.com/image.png</property>
		<property id="content" type="string">Hello, World!</property></properties>
	<settings>
		<setting propertyKey="@Properties.webhookUrl" title="@Strings.webhookUrl">
			<settingConfig type="url" />
		</setting>
		<setting propertyKey="@Properties.username" title="@Strings.username">
			<settingConfig type="alphaNumeric" />
		</setting>
		<setting propertyKey="@Properties.avatarUrl" title="@Strings.avatarUrl">
			<settingConfig type="url" />
		</setting>
		<setting propertyKey="@Properties.content" title="@Strings.content">
			<settingConfig type="alphaNumeric" />
		</setting>
	</settings>
</resources>